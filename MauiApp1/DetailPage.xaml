<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.DetailPage">
  
    <ContentPage.Content>
        <VerticalStackLayout
            Spacing="25"
            Padding="30,0"
            VerticalOptions="Center">
            <Label>Detail</Label>
            <Button Clicked="NavigateBack" Text="GoToAsync('..')"></Button>
            <Button Clicked="NavigateBackPop" Text="Navigation.PopAsync()"></Button>
        </VerticalStackLayout>
    </ContentPage.Content>
</ContentPage>